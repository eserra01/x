<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="pabs_product_expense_custom" model="ir.ui.view">
    <field name="name">pabs.product.expense.custom</field>
    <field name="model">product.product</field>
    <field name="inherit_id" ref="hr_expense.product_product_expense_form_view"/>
    <field name="arch" type="xml">     
      <xpath expr="//group[@name='product_details']" position="after">
        <group>
           <field name="requires_plate" />
        </group>       
      </xpath>
    </field>
  </record>
</odoo>