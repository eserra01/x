<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="show_digital_images_wizard_form_view" model="ir.ui.view">
        <field name="name">show.digital.images.wizard.form.view</field>
        <field name="model">show.digital.images.wizard</field>
        <field name="arch" type="xml">
            <form string="Mostrar imagenes">
                <sheet>
                    <group>                    
                        <field name="url_ine" readonly="True" widget="url"/>
                        <field name="url_comprobante_domicilio" readonly="True" widget="url" />
                        <field name="url_fachada_domicilio" readonly="True" widget="url" />
                        <field name="url_contrato_reafiliacion" readonly="True" widget="url"/>
                    </group>
                </sheet>
                <footer>                    
                    <button special="cancel" string="Cerrar" class="oe_link"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
