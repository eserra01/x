<?xml version="1.0" encoding="utf-8"?>
<odoo>
  
  <record id="pabs_stock_status_tree_view" model="ir.ui.view">
    <field name="name">Solicitudes</field>
    <field name="type">tree</field>
    <field name="model">pabs.status.stock</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="code"/>
        <field name="employee_id"/>
        <field name="office_code"/>
        <field name="office_id"/>
      </tree>
    </field>
  </record>

  <record id="pabs_stock_status_form_view" model="ir.ui.view">
    <field name="name">Busqueda Solciitud</field>
    <field name="type">form</field>
    <field name="model">pabs.status.stock</field>
    <field name="arch" type="xml">
      <form>
        <header>
          
        </header>
        <sheet>
          
        </sheet>
      </form>
    </field>
  </record>

  <record id="pabs_status_stock_action" model="ir.actions.act_window">
    <field name="name">Busqueda Solciitud</field>
    <field name="res_model">pabs.status.stock</field>
    <field name="view_mode">tree,form</field>
  </record>


</odoo>