<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="pabs_payment_scheme_tree_view" model="ir.ui.view">
    <field name="name">Esquemas de Pago</field>
    <field name="type">tree</field>
    <field name="model">pabs.payment.scheme</field>
    <field name="arch" type="xml">
      <tree editable="top">
        <field name="allow_all"/>
        <field name="name"/>
        <field name="description"/>
      </tree>
    </field>
  </record>

  <record id="payment_scheme_action" model="ir.actions.act_window">
    <field name="name">Esquema de Pagos</field>
    <field name="res_model">pabs.payment.scheme</field>
    <field name="view_mode">tree</field>
  </record>

</odoo>