<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="pabs_recluitment_origin_tree_view" model="ir.ui.view">
    <field name="name">Origen de reclutamiento</field>
    <field name="type">tree</field>
    <field name="model">pabs.recluitment.origin</field>
    <field name="arch" type="xml">
      <tree editable="top">
        <field name="name"/>
        <field name="description"/>
      </tree>
    </field>
  </record>

  <record id="recluitment_origin_action" model="ir.actions.act_window">
    <field name="name">Origen de reclutamiento</field>
    <field name="res_model">pabs.recluitment.origin</field>
    <field name="view_mode">tree</field>
  </record>

</odoo>