<odoo>
  <data>

    <record id="ig_templo_form_view" model="ir.ui.view">
      <field name="name">ig.templo.form.view</field>
      <field name="model">ig.templo</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>

              <group>
                <field name="name" />
              </group>

            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="ig_templo_tree_view" model="ir.ui.view">
      <field name="name">ig.templo.tree.view</field>
      <field name="model">ig.templo</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name" />
        </tree>
      </field>
    </record>

    <record id="action_ig_templo" model="ir.actions.act_window">
      <field name="name">Templo</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">ig.templo</field>
      <field name="view_mode">tree,form</field>
    </record>

  </data>
</odoo>
