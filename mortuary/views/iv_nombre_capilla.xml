<odoo>
  <data>

    <record id="iv_nombre_capilla_form_view" model="ir.ui.view">
      <field name="name">iv.nombre.capilla.form.view</field>
      <field name="model">iv.nombre.capilla</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>

              <group>
                <field name="name" />
              </group>

            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="iv_nombre_capilla_tree_view" model="ir.ui.view">
      <field name="name">iv.nombre.capilla.tree.view</field>
      <field name="model">iv.nombre.capilla</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name" />
        </tree>
      </field>
    </record>

    <record id="action_iv_nombre_capilla" model="ir.actions.act_window">
      <field name="name">Nombre de capilla</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">iv.nombre.capilla</field>
      <field name="view_mode">tree,form</field>
    </record>

  </data>
</odoo>
